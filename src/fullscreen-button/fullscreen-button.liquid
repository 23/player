{% if supportsFullscreen %}
  <div class="button-container">
    <button class="tray-button button fullscreen-button {% if fullscreen %}fullscreen-button-off{% else %}fullscreen-button-on{% endif %}" click="$toggle:fullscreen" aria-label="{{"toggle_fullscreen"|translate}}" label="{{"toggle_fullscreen"|translate}}" tabindex="1800">
    
      <span class="hide-visually">
        {{"toggle_fullscreen"|translate}}
      </span>
      
  </button>
</div>
{% endif %}
