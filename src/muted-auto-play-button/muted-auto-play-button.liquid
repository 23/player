{% if playing and volumeMuted and mutedAutoPlay and ambient == false %}
  <div class="muted-auto-play muted-auto-play-{{unmuteButtonPosition|escape}}">
    <div
      click="$toggle:mutedAutoPlay"
      class="muted-auto-play-button muted-auto-play-button-{{unmuteButtonPosition|escape}}"
      aria-label="{{"muted-autoplay"|translate}}"
      role="button"
      tabindex="0">
      Click to unmute
    </div>
  </div>
{% endif %}